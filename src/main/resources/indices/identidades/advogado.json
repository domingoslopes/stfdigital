{
  "settings": {
    "analysis": {
      "analyzer": {
        "keyword_analyzer": {
          "type": "custom",
          "tokenizer": "keyword",
          "filter": [ "lowercase" ]
        }
      }
    }
  },
  "mappings": {
    "Advogado": {
      "dynamic": false,
      "properties": {
        "pessoaId": {
          "properties": {
            "sequencial": {
              "type": "long",
          	  "index": "not_analyzed",
          	  "include_in_all": false
            }
          }
        },
        "nome": {
          "type": "string",
          "analyzer": "keyword_analyzer"
        },
        "oab": {
          "type": "string",
          "index": "not_analyzed"
        },
        "cpf": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  }
}