{
    "template": "monitoring-method-*",
    "mappings": {
        "MethodCall": {
            "_ttl" : { "enabled" : true, "default": "12w" },
            "properties": {
            	"monitorGroup": {
                    "type": "string",
                    "index": "not_analyzed"
                },
            	"@timestamp": {
                    "type": "date",
                    "format": "dateOptionalTime"
                },
                "clazz": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "method": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "resourceId": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "resourceSize": {
                    "type": "long",
                    "index": "not_analyzed"
                },
                "durationInMillis": {
                    "type": "long",
                    "index": "not_analyzed"
                },
                "errorOcurred": {
                    "type": "boolean",
                    "index": "not_analyzed"
                }
            }
        }
    }
}