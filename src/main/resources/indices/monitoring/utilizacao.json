{
    "template": "utilizacao-*",
    "mappings": {
        "RequestTrace": {
            "_ttl" : { "enabled" : true, "default": "12w" },
            "properties": {
                "@timestamp": {
                    "type": "date",
                    "format": "dateOptionalTime"
                },
                "method": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "url": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "usuario": {
                    "type": "string",
                    "index": "not_analyzed"
                },
                "performance": {
                    "type": "long",
                    "index": "not_analyzed"
                },
                "status": {
                    "type": "long",
                    "index": "not_analyzed"
                },
                "causa": {
                    "type": "string",
                    "index": "not_analyzed"
                }
            }
        }
    }
}